	<queryString>
		<![CDATA[SELECT DISTINCT
    A1.ID_TIPO_INSUMO   AS ID_TIPO_INSUMO,
    A1.NOMBRE           AS NOMBRE
FROM
    (
        SELECT
            A3.ID_TIPO_INSUMO   AS ID_TIPO_INSUMO,
            A3.FECHA            AS FECHA,
            A2.NOMBRE           AS NOMBRE
        FROM
            PPOS.INSUMO        A3,
            PPOS.TIPO_INSUMO   A2
        WHERE
            A2.ID = A3.ID_TIPO_INSUMO
    ) A1
WHERE
    A1.FECHA >= $P{fecInicio} AND
    A1.FECHA < ADD_MONTHS(TO_DATE($P{fecFin},'DD/MM/YY'), 1) AND
    (A1.ID_TIPO_INSUMO = $P{idTipoInsumo} OR $P{idTipoInsumo} = 0)]]>
	</queryString>
