	<queryString>
		<![CDATA[SELECT DISTINCT
    A1.ID_PRODUCTO   AS ID_PRODUCTO,
    A1.NOMBRE        AS NOMBRE,
    (SELECT M.NOMBRE FROM MAESTRA M WHERE M.ID=A1.IDT_UNIDAD_MEDIDA) AS NOM_UNIDAD_MEDIDA
FROM
    (
        SELECT
            A3.ID_PRODUCTO          AS ID_PRODUCTO,
            A2.FEC_USUARIO_CREA     AS FECHA,
            A4.NOMBRE               AS NOMBRE,
            A4.IDT_UNIDAD_MEDIDA    AS IDT_UNIDAD_MEDIDA
        FROM
            PPOS.VENTA        A2,
            PPOS.DETALLE_VENTA   A3,
            PPOS.PRODUCTO A4
        WHERE
            A3.ID_VENTA = A2.ID AND
            A4.ID = A3.ID_PRODUCTO
    ) A1
WHERE
    A1.FECHA >= $P{fecInicio} AND
    A1.FECHA < ADD_MONTHS(TO_DATE($P{fecFin},'DD/MM/YY'), 1) AND
    (A1.ID_PRODUCTO = $P{idProducto} OR $P{idProducto} = 0)]]>
	</queryString>
