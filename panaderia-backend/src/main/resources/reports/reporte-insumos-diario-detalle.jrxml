	</parameter>
	<parameter name="nomTipoInsumo" class="java.lang.String"/>
	<parameter name="idPersonal" class="java.lang.Long"/>
	<parameter name="fecInicio" class="java.util.Date"/>
	<parameter name="fecFin" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
(PE.NOMBRE || ' ' || PE.APE_PATERNO || ' ' || PE.APE_MATERNO) AS NOMBRE_COMPLETO,
I.FECHA AS FECHA,
SUM(I.CANTIDAD) AS SUMA
FROM INSUMO I
INNER JOIN PERSONAL P ON P.ID = I.ID_PERSONAL
INNER JOIN PERSONA PE ON PE.ID = P.ID_PERSONA
WHERE I.ID_TIPO_INSUMO = $P{idTipoInsumo} AND (I.ID_PERSONAL = $P{idPersonal} OR $P{idPersonal} = 0) AND I.FECHA>=$P{fecInicio} AND I.FECHA<=$P{fecFin}
GROUP BY
(PE.NOMBRE || ' ' || PE.APE_PATERNO || ' ' || PE.APE_MATERNO),
I.FECHA
ORDER BY NOMBRE_COMPLETO, I.FECHA DESC]]>
	</queryString>
	<field name="NOMBRE_COMPLETO" class="java.lang.String"/>
	<field name="FECHA" class="java.sql.Timestamp"/>
	<field name="SUMA" class="java.math.BigDecimal"/>
